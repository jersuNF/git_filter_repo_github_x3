	int "Maximum horizontal dilution of precision in dm"
	default 200

config GNSS_HACC_LIMIT
	int "Maximum horizontal accuracy in dm"
	default 35

config GNSS_CACC_LIMIT
	int "Maximum course heading accuracy in centidegrees, deg*100"
	default 36000